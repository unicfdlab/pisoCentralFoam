FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pressureBound
{
    type            limitScalarField;
    active          yes;

    limitScalarFieldCoeffs
    {
        selectionMode   all;
        fieldName       p;
        minFieldValue   1.0e-4;
        scaleCoeff      0.9;
    }
}




