FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    inlet
    {
	type		zeroGradient;
/*        type            totalPressure;
        phi             phi;
        rho             none;
        psi             psi;
        U               Uabs;
        gamma           1.4;
        p0              uniform 1.4e5;
        value           $internalField;*/
    }

    outlet
    {


        type            fixedValue;
        value           $internalField;
    }

    walls
    {
        type            zeroGradient;
    }
    topAndBottom
	{	
		type	empty;
	}
}

